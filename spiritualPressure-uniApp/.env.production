# 生产环境配置
NODE_ENV=production

# API 基础地址
VITE_API_BASE_URL=https://api.yourdomain.com/api
VITE_APP_BASE_URL=https://www.yourdomain.com

# 应用配置
VITE_APP_TITLE=SpiritualPressure
VITE_APP_VERSION=1.0.0

# 第三方服务配置
VITE_WEIXIN_APP_ID=wx_production_app_id
VITE_ALIPAY_APP_ID=alipay_production_app_id

# 调试配置
VITE_DEBUG=false
VITE_LOG_LEVEL=error

# 功能开关
VITE_ENABLE_MOCK=false
VITE_ENABLE_ANALYTICS=true

# 地图服务
VITE_MAP_API_KEY=production_map_key

# 文件上传配置
VITE_UPLOAD_MAX_SIZE=20
VITE_UPLOAD_ALLOWED_TYPES=image/jpeg,image/png,image/gif

# WebSocket配置
VITE_WS_URL=wss://api.yourdomain.com/ws

# CDN配置
VITE_CDN_BASE_URL=https://cdn.yourdomain.com

# 性能优化配置
VITE_ENABLE_GZIP=true
VITE_ENABLE_BROTLI=true