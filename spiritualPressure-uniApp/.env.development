# 开发环境配置
NODE_ENV=development

# API 基础地址
VITE_API_BASE_URL=http://localhost:3000/api
VITE_APP_BASE_URL=http://localhost:3000

# 应用配置
VITE_APP_TITLE=SpiritualPressure - 开发环境
VITE_APP_VERSION=1.0.0-dev

# 第三方服务配置
VITE_WEIXIN_APP_ID=wx_development_app_id
VITE_ALIPAY_APP_ID=alipay_development_app_id

# 调试配置
VITE_DEBUG=true
VITE_LOG_LEVEL=debug

# 功能开关
VITE_ENABLE_MOCK=true
VITE_ENABLE_ANALYTICS=false

# 地图服务
VITE_MAP_API_KEY=development_map_key

# 文件上传配置
VITE_UPLOAD_MAX_SIZE=10
VITE_UPLOAD_ALLOWED_TYPES=image/jpeg,image/png,image/gif

# WebSocket配置
VITE_WS_URL=ws://localhost:3000/ws